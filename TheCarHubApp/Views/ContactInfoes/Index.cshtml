@model IEnumerable<TheCarHubApp.Data.ContactInfo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AddressLine1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AddressLine2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Zip)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WeekDayOpeningHour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WeekDayClosingHour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SaturdarOpeningHour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SaturdayClosingHour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MapLat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MapLong)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MapZoom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MapTitle)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AddressLine1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddressLine2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Zip)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WeekDayOpeningHour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WeekDayClosingHour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SaturdarOpeningHour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SaturdayClosingHour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MapLat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MapLong)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MapZoom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MapTitle)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
